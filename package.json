{
  "name": "git-branching-wiki",
  "version": "0.5.0",
  "description": "A wiki system that generates a static wiki site based on git branches, for easy documentation of various branches of your codebase, allowing your documentation to follow the same branching model that your codebase does.",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- -R spec 'src/tests/**/*Test.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/silvermine/git-branching-wiki.git"
  },
  "keywords": [
    "git",
    "wiki",
    "branches",
    "documentation"
  ],
  "author": "Jeremy Thomerson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/silvermine/git-branching-wiki/issues"
  },
  "homepage": "https://github.com/silvermine/git-branching-wiki#readme",
  "dependencies": {
    "cheerio": "0.20.0",
    "chokidar": "1.4.3",
    "class.extend": "0.9.2",
    "marked": "0.3.5",
    "metalsmith": "2.1.0",
    "mkdirp": "0.5.1",
    "nodegit": "0.11.9",
    "nunjucks": "2.4.1",
    "q": "1.4.1",
    "recursive-copy": "2.0.3",
    "underscore": "1.8.3"
  },
  "devDependencies": {
    "coveralls": "2.11.9",
    "expect.js": "0.3.1",
    "grunt-contrib-jshint": "1.0.0",
    "istanbul": "0.4.2",
    "mocha": "2.4.5",
    "mocha-lcov-reporter": "1.2.0",
    "yaml-front-matter": "3.4.0"
  }
}
